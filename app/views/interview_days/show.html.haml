- @interview_day.interviews.each do |interview|
  .jumbotron{class: interview.status}
    - applicant = interview.applicant
    %h2= interview
    - if admin?
      %h4
        Interview at:
        =(interview.start_time + 2.hours + 30.minutes).strftime("%H:%M")
    %ul
      %li= interview.status
      - unless applicant.nil?
        %li= link_to applicant.name, applicant_path(applicant.key)
        %li= link_to applicant.nick, applicant_path(applicant.key)
        %li= link_to applicant.email, applicant_path(applicant.key)
    -if applicant
      %pre= applicant.essay
      %pre= Settings.questionary_notes


